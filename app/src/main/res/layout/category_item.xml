<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <com.google.android.material.card.MaterialCardView
        android:id="@+id/linearEngland"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_10pxh"
        android:layout_marginTop="@dimen/_10pxh"
        android:layout_marginEnd="@dimen/_10pxh"
        android:gravity="center_horizontal"
        android:orientation="horizontal"
        app:cardCornerRadius="@dimen/_12pxh">

        <LinearLayout
            android:id="@+id/linearRowunlimitedannuaOne"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            android:layout_marginTop="@dimen/_10pxv"

            android:layout_marginBottom="@dimen/_10pxv"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/txtUnlimitedannuaOne"
                style="@style/txtInterregular12"
                android:layout_width="0dp"
                android:layout_height="@dimen/_25pxh"
                android:layout_weight="0.2"
                android:ellipsize="end"
                android:lineSpacingExtra="@dimen/_1pxh"
                android:singleLine="true"
                android:src="@drawable/category64"
                android:text="Requested  To     :" />

            <TextView
                android:id="@+id/requester"
                style="@style/txtInterregular12"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="0.8"
                android:gravity="center_vertical"
                android:ellipsize="end"
                android:lineSpacingExtra="@dimen/_5pxh"
                android:singleLine="false"
                android:text="jinushan" />
        </LinearLayout>
    </com.google.android.material.card.MaterialCardView>
</layout>
